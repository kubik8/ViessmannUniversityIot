<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<appSettings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <add key="ServiceEndpoints:Users" value="staging_users_endpoint_address"
     xdt:Locator="Match(key)"
     xdt:Transform="Replace"/>
  <add key="ServiceEndpoints:Gateways" value="staging_gateways_endpoint_address"
     xdt:Locator="Match(key)"
     xdt:Transform="Replace"/>
</appSettings>